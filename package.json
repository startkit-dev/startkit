{
  "name": "@startkit-dev/startkit",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build",
    "check": "bun run format:check && bun run lint && bun run typecheck",
    "clean": "git clean -xdf node_modules/.cache .tanstack .output .nitro",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:reset": "git clean -xdf sqlite.db && bun db:migrate",
    "db:studio": "drizzle-kit studio",
    "dev": "vite dev",
    "fix": "bun run format && bun run lint:fix",
    "format": "prettier --experimental-cli --write .",
    "format:check": "prettier --experimental-cli --check .",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "nuke": "bun run clean && git clean -xdf node_modules",
    "outdated": "npx npm-check-updates --interactive --format group",
    "start": "vite start",
    "typecheck": "tsc --noEmit --pretty"
  },
  "dependencies": {
    "@fontsource-variable/geist": "^5.2.6",
    "@fontsource-variable/geist-mono": "^5.2.6",
    "@libsql/client": "^0.15.10",
    "@tanstack/react-query": "^5.84.2",
    "@tanstack/react-query-devtools": "^5.84.2",
    "@tanstack/react-router": "^1.130.12",
    "@tanstack/react-router-devtools": "^1.130.13",
    "@tanstack/react-router-with-query": "^1.130.12",
    "@tanstack/react-start": "^1.130.16",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.44.4",
    "lucide-react": "^0.539.0",
    "radix-ui": "^1.4.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.0.16"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.6.1",
    "@prettier/plugin-oxc": "^0.0.4",
    "@tailwindcss/vite": "^4.1.11",
    "@tanstack/eslint-plugin-router": "^1.130.12",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react-oxc": "^0.4.0",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9.33.0",
    "eslint-config-flat-gitignore": "^2.1.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import-x": "^4.16.1",
    "prettier": "^3.6.2",
    "prettier-plugin-jsdoc": "^1.3.3",
    "prettier-plugin-packagejson": "^2.5.19",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4.1.11",
    "tw-animate-css": "^1.3.6",
    "typescript": "^5",
    "typescript-eslint": "^8.39.0",
    "vite": "npm:rolldown-vite@latest",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
